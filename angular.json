{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "todo-app": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": { "style": "scss" },
        "@schematics/angular:application": { "strict": true }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/todo-app",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": ["src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "2mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": { "browserTarget": "todo-app:build:production" },
            "development": { "browserTarget": "todo-app:build:development" }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": { "browserTarget": "todo-app:build" }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": ["src/styles.scss"],
            "scripts": []
          }
        },
        "deploy": {
          "builder": "@angular/fire:deploy",
          "options": {
            "prerender": false,
            "ssr": false,
            "browserTarget": "todo-app:build:production",
            "firebaseProject": "todo-app-6c693",
            "firebaseHostingSite": "todo-app-6c693"
          }
        }
      }
    },
    "add-task": {
      "projectType": "library",
      "root": "projects/add-task",
      "sourceRoot": "projects/add-task/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": { "project": "projects/add-task/ng-package.json" },
          "configurations": {
            "production": {
              "tsConfig": "projects/add-task/tsconfig.lib.prod.json"
            },
            "development": { "tsConfig": "projects/add-task/tsconfig.lib.json" }
          },
          "defaultConfiguration": "production"
        }
      }
    },
    "date": {
      "projectType": "library",
      "root": "projects/date",
      "sourceRoot": "projects/date/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": { "project": "projects/date/ng-package.json" },
          "configurations": {
            "production": {
              "tsConfig": "projects/date/tsconfig.lib.prod.json"
            },
            "development": { "tsConfig": "projects/date/tsconfig.lib.json" }
          },
          "defaultConfiguration": "production"
        }
      }
    },
    "photo": {
      "projectType": "library",
      "root": "projects/photo",
      "sourceRoot": "projects/photo/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": { "project": "projects/photo/ng-package.json" },
          "configurations": {
            "production": {
              "tsConfig": "projects/photo/tsconfig.lib.prod.json"
            },
            "development": { "tsConfig": "projects/photo/tsconfig.lib.json" }
          },
          "defaultConfiguration": "production"
        }
      }
    },
    "task": {
      "projectType": "library",
      "root": "projects/task",
      "sourceRoot": "projects/task/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": { "project": "projects/task/ng-package.json" },
          "configurations": {
            "production": {
              "tsConfig": "projects/task/tsconfig.lib.prod.json"
            },
            "development": { "tsConfig": "projects/task/tsconfig.lib.json" }
          },
          "defaultConfiguration": "production"
        }
      }
    }
  },
  "defaultProject": "todo-app"
}
